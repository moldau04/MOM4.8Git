                }

                var res = TestTypeCover.split(",");
                var mycombobox = $find("<%= ddlTestTypeCover.ClientID %>");

                var flag = false;
                for (var i = 0; i < mycombobox.get_items().get_count(); i++) {
                    var flag = false;
                    mycombobox.get_items().getItem(i).set_checked(false);
                    mycombobox.get_items().getItem(i).set_enabled(true);
                    var v = mycombobox.get_items().getItem(i).get_value();
                    for (var j = 0; j < res.length; j++) {
                        if (v == res[j].trim()) {
                            flag = true;
                        }
                    }
                    if (flag == true) {
                        mycombobox.get_items().getItem(i).set_checked(true);
                    }
                    if (v == ID) {
                        mycombobox.get_items().getItem(i).set_enabled(false);
                    }
                }

                mycombobox.commitChanges();
                var combo = $('#<%=ddlTestTypeCover.ClientID%>');


                var wnd = $find('<%=TestTypeWindow.ClientID %>');
