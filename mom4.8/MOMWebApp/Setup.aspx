        function onChangeddlTypeStage(control) {
            var value = $(control).val();
            if (value === "Close/Won") {
                $(".txtProbability").val("100");
            }
            else if (value === "Close/Out") {
                $(".txtProbability").val("0");
            }
            else if (value === "Open") {
                $(".txtProbability").val("");
            }
            Materialize.updateTextFields();
        }

        function onChangeProbability(control) {
            var value = $(control).val();
            $(control).val(parseInt(value));
            if (parseInt(value) === 100) {
                $("[id$='ddlTypeStage'] option").removeAttr("selected");
                $("[id$='ddlTypeStage']").val("Close/Won");
            }
            else if (parseInt(value) === 0) {
                $("[id$='ddlTypeStage'] option").removeAttr("selected");
                $("[id$='ddlTypeStage']").val("Close/Out");
            }
            else {
                $("[id$='ddlTypeStage'] option").removeAttr("selected");
                $("[id$='ddlTypeStage']").val("Open");
            }

            if (parseInt(value) > 100 || parseInt(value) < 0) {
                noty({
                    text: 'The probability value is invalid.',
                    type: 'warning',
                    layout: 'topCenter',
                    closeOnSelfClick: false,
                    timeout: 5000,
                    theme: 'noty_theme_default',
                    closable: true
                });
                $("[id$='ddlTypeStage'] option").removeAttr("selected");
                $("[id$='ddlTypeStage']").val("Open");
                $(".txtProbability").val("");
                return false;
            }
        }
