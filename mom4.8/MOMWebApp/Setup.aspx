        function OpenStageProjectAddModal() {
            $('#<%=txtDescriptionProject.ClientID%>').val("");
            var colorPicker = $find("<%= txtChartColorProject.ClientID %>");
            colorPicker.set_selectedColor("");
            $("[id$='txtChartColorProject_icon']").removeAttr("style");

            return true;
            <%--var wnd = $find('<%=StageProjectWindow.ClientID %>');
            wnd.set_title("Add Stage");
            wnd.Show();
            Materialize.updateTextFields();--%>
        }
        function OpenStageProjectEdit() {
            var grid = $find("<%=RadGrid_StageProject.ClientID %>");
            var MasterTable = grid.get_masterTableView();
            var selectedRows = MasterTable.get_selectedItems();
            var ID = "";
            var Description = "";
            var chartColor = "";
            for (var i = 0; i < selectedRows.length; i++) {
                var row = selectedRows[i];
                ID = MasterTable.getCellByColumnUniqueName(row, "lblStageId").innerHTML.replace(/&nbsp;/g, '');
                Description = MasterTable.getCellByColumnUniqueName(row, "lblDescription").innerHTML.replace(/&nbsp;/g, '');
                chartColor = $($(MasterTable.getCellByColumnUniqueName(row, "lblChartColor"))[0]).find(".invisibleGridTd").text();
            }

            if (ID != "") {
                $('#<%=txtDescriptionProject.ClientID%>').val(Description);
                var colorPicker = $find("<%= txtChartColorProject.ClientID %>");
                if (chartColor === "") {
                    $("[id$='txtChartColorProject_icon']").removeAttr("style");
                    colorPicker.set_selectedColor("");
                }
                else {
                    colorPicker.set_selectedColor("#" + chartColor + "");

                }



                return true;
            }
            else {
                ChkWarning();
                return false;
            }
        }

