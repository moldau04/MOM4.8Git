        function OpenCallCodesEdit() {
            var grid = $find("<%=RadGrid_CallCodes.ClientID %>");
            var MasterTable = grid.get_masterTableView();
            var selectedRows = MasterTable.get_selectedItems();
            var CodeCategory = "";
            var Description = "";
            var Type = "";

            for (var i = 0; i < selectedRows.length; i++) {
                var row = selectedRows[i];
                var Category = MasterTable.getCellByColumnUniqueName(row, "lblCat").innerHTML;
                CodeCategory = $(Category).html();
                var CodeDescription = MasterTable.getCellByColumnUniqueName(row, "lblCDescription").innerHTML;
                Description = $(CodeDescription).html();
                var CodeType = MasterTable.getCellByColumnUniqueName(row, "lblTypeID").innerHTML;
                Type = $(CodeType).html();
                if (Type == "Resolution") {
                    Type = 1;
                }
                else { Type = 0; }
            }
            if (Description != "") {
                $('#<%=txtCodeDesc.ClientID%>').prop("readonly", true);
                $('#<%=ddlCodeCategory.ClientID%>').prop("disabled", false);
                $('#<%=ddlCodeType.ClientID%>').prop("disabled", false);
                $('#<%=txtCodeDesc.ClientID%>').val(Description);
