        function OpenViolationCodeWindow() {
            $('#<%=txtViolationCodeName.ClientID%>').val("");
            $('#<%=hdnViolationCodeID.ClientID%>').val("0");
            $('#<%=txtViolationCodeDesc.ClientID%>').val("");
            $('#<%=ddlViolationCodeSection.ClientID%>').val("0");
            $('#<%=ddlViolationCodeCategory.ClientID%>').val("0");

            var wnd = $find('<%=ViolationCodeWindow.ClientID %>');
            wnd.set_title("Violation Code");
            wnd.set_height(424);
            wnd.Show();
        }
        function OpenViolationCodeWindowEdit() {
            var ID = "";
            var Code = "";
            var Desc = "";
            var SectionID = "";
            var CategoryID = "";

            $("#<%=RadGrid_ViolationCode.ClientID %>").find('tr:not(:first,:last)').each(function () {
                var $tr = $(this);
                $tr.find('input[type="checkbox"]:first:checked').each(function (index, value) {

                    Desc = $tr.find('span[id*=lblDesc]').text();
                    Code = $tr.find('span[id*=lblCode]').text();
                    ID = $tr.find('input[id*=hdnid]').val();
                    SectionID = $tr.find('input[id*=hdnSectionID]').val();
                    CategoryID = $tr.find('input[id*=hdnCategoryID]').val();
                });
            });
            if (ID != "") {
                $('#<%=txtViolationCodeName.ClientID%>').val(Code);
                $('#<%=txtViolationCodeDesc.ClientID%>').val(Desc);
                $('#<%=hdnViolationCodeID.ClientID%>').val(ID);
                $('#<%=ddlViolationCodeSection.ClientID%>').val(SectionID);
                $('#<%=ddlViolationCodeCategory.ClientID%>').val(CategoryID);
                var wnd = $find('<%=ViolationCodeWindow.ClientID %>');
                wnd.set_title("Edit Violation Code");

                wnd.set_height(500);
                wnd.Show();

                Materialize.updateTextFields();
            }
            else {
                ChkWarning();
            }
        }

        function CloseViolationCodeWindow() {
            var wnd = $find('<%=ViolationCodeWindow.ClientID %>');
            wnd.Close();
        }

        function OpenViolationSectionWindow() {

            var wnd = $find('<%=ViolationSectionWindow.ClientID %>');
            wnd.set_title("Violation Section");
            wnd.set_height(200);
            wnd.Show();
        }

        function CloseViolationSectionWindow() {
            var wnd = $find('<%=ViolationSectionWindow.ClientID %>');
            wnd.Close();

            try {
                var count = $('#<%=ddlViolationCodeSection.ClientID%>')[0].length;

                $('#<%=ddlViolationCodeSection.ClientID%>').val(count - 1)
            } catch (e) {

            }

            $('#<%=ddlViolationCodeSection.ClientID%>').focus();
        }
        function showAddNewSection(obj) {
            if (obj.selectedIndex == "0") {
                OpenViolationSectionWindow();
            }
        }

        function OpenViolationCategoryWindow() {

            var wnd = $find('<%=ViolationCategoryWindow.ClientID %>');
            wnd.set_title("Violation Category");
            wnd.set_height(200);
            wnd.Show();
        }

        function CloseViolationCategoryWindow() {
            var wnd = $find('<%=ViolationCategoryWindow.ClientID %>');
            wnd.Close();

            try {
                var count = $('#<%=ddlViolationCodeCategory.ClientID%>')[0].length;

                $('#<%=ddlViolationCodeCategory.ClientID%>').val(count - 1)
            } catch (e) {

            }

            $('#<%=ddlViolationCodeCategory.ClientID%>').focus();
        }
        function showAddNewCategory(obj) {

            if (obj.selectedIndex == "0") {
                OpenViolationCategoryWindow();
            }
