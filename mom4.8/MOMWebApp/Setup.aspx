        function OpenCategoryScheduleEdit() {
            $('#<%=hddCategoryAction.ClientID%>').val("1");
            var grid = $find("<%=RadGrid_CategorySchedule.ClientID %>");
            var MasterTable = grid.get_masterTableView();
            var selectedRows = MasterTable.get_selectedItems();
            var ID = "";
            var ImageID = "";
            var Remarks = "";
            var Charge = "";
            var isActive = "";
            var Default = "";
            var ImagePath = "";
            for (var i = 0; i < selectedRows.length; i++) {
                var row = selectedRows[i];
                ID = MasterTable.getCellByColumnUniqueName(row, "lblCType").innerHTML.replace(/&amp;/g, '&');
                ImageID = ID.replace(/&/g, '$');
                Remarks = MasterTable.getCellByColumnUniqueName(row, "lblCRemarks").innerHTML.replace(/&nbsp;/g, '').replace(/&amp;/g, '&');
                Charge = MasterTable.getCellByColumnUniqueName(row, "lblCharge").innerHTML;
                Charge = $(Charge).html();
                isActive = MasterTable.getCellByColumnUniqueName(row, "lblActive").innerHTML;
                isActive = $(isActive).html();
                Default = MasterTable.getCellByColumnUniqueName(row, "lbldefault").innerHTML.replace(/&nbsp;/g, '');
                Default = $(Default).html();
                ImagePath = "imagehandler.ashx?catid=" + ImageID;
            }
            if (ID != "") {
                $('#<%=txtCat.ClientID%>').prop("readonly", true);
                $('#<%=txtCat.ClientID%>').val(ID);
                $('#<%=txtCatRem.ClientID%>').val(Remarks);
                if (Default != "") { document.getElementById('<%= chkDefaultCat.ClientID %>').checked = true; }
                else { document.getElementById('<%= chkDefaultCat.ClientID %>').checked = false; }
                if (Charge == "True") { document.getElementById('<%= chkChargeable.ClientID %>').checked = true; }
                else { document.getElementById('<%= chkChargeable.ClientID %>').checked = false; }
                if (isActive == "Active") { $("#<%= ddlCategoryStatus.ClientID %>").val("1"); }
                else { $("#<%= ddlCategoryStatus.ClientID %>").val("0"); }
                $("[id$='imgCatIcon']").attr("src", ImagePath);
