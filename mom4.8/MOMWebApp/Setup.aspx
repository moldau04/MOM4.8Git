        $(document).ready(function () {

            var tab = GetQueryStringParams('tabWarehouse');
            var tab1 = GetQueryStringParams('tab');
            if (tab == "Warehouse") {
                $('#accrdinventorysetup').addClass('active');
                $('#dvWarehouse').show();
                $('#accrdcustomersetup').removeClass('active');
                $('#dvCustomerSetup').attr('style', 'display:none');
            }
            else if (tab1 == "1") {
                $('#accrdprojectsetup').addClass('active');
                $('#dvDepartment').show();
                $('#accrdcustomersetup').removeClass('active');
                $('#dvCustomerSetup').attr('style', 'display:none');
            }
            else {
                $(document).ready(function () {
                    $('#accrdcustomersetup').addClass('active');
                    $('#defaultcustomersetup').addClass('anchorActive');
                    $('#dvCustomerSetup').attr('style', 'display:block');
                    $('#dvCustomerSetup').show();
                });
            }
        });
        function GetQueryStringParams(sParam) {
            var sPageURL = window.location.search.substring(1);
            var sURLVariables = sPageURL.split('?');
            for (var i = 0; i < sURLVariables.length; i++) {
                var sParameterName = sURLVariables[i].split('=');
                if (sParameterName[0] == sParam) {
                    return sParameterName[1];
                }
            }
        }
